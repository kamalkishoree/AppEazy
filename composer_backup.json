        "dg/mysql-dump": "^1.5",
        "doctrine/dbal": "^3.1",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "genealabs/laravel-sign-in-with-apple": "^0.5.0",
        "genealabs/laravel-socialiter": "^0.3.3",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "hubspot/hubspot-php": "*",
        "intervention/image": "^2.5",
        "jackiedo/timezonelist": "5.*",
        "jenssegers/agent": "^2.6",
        "jorenvanhocht/laravel-share": "^4.1",
        "kawankoding/laravel-fcm": "^0.2.0",
        "kreait/laravel-firebase": "^3.1",
        "kutia-software-company/larafirebase": "^1.3",
        "laravel-notification-channels/fcm": "~2.0",
        "laravel/framework": "^8.0",
        "laravel/scout": "^9.0",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.0",
        "laravelcollective/html": "^6.2",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/glide-laravel": "^1.0",
        "league/omnipay": "^3",
        "maatwebsite/excel": "^3.0.1",
        "mobbex/sdk": "0.1.0",
        "multicaret/laravel-unifonic": "^2.0",
        "nikolag/laravel-square": "2.6",
        "nyholm/psr7": "^1.5",
        "omnipay/payfast": "^3.0",
        "omnipay/paypal": "^3.0",
        "omnipay/stripe": "^3.1",
        "openpay/sdk": "^3.0",
        "orangehill/iseed": "^3.0",
        "owen-it/laravel-auditing": "^13.0",
        "pagarme/pagarme-php": "^4.1",
        "pallares/laravel-nuxt": "^1.5",
        "passbase/passbase-php": "^1.3",
        "paylink/paylink": "^2.0",
        "paystackhq/omnipay-paystack": "^1.0",
        "paytabscom/laravel_paytabs": "^1.1",
        "predis/predis": "^1.1",
        "r0bdiablo/laravel5-phumbor": "^1.0",
        "rak/simplify": "dev-master",
        "rap2hpoutre/laravel-log-viewer": "^1.7",
        "razorpay/razorpay": "2.*",
        "rivalex/laravel-intl": "^1.1",
        "rossjcooper/laravel-hubspot": "^4.0",
        "sentry/sentry-laravel": "^2.7",
        "shippo/shippo-php": "^1.5",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "socialiteproviders/apple": "^5.0",
        "spatie/db-dumper": "^2.21",
        "spatie/geocoder": "^3.10",
        "spatie/laravel-permission": "^5.7",
        "spatie/laravel-webhook-client": "^2.7",
        "spatie/laravel-webhook-server": "^2.0",
        "square/square": "25.1.0.20230119",
        "staudenmeir/laravel-adjacency-list": "^1.0",
        "stripe/stripe-php": "^7.100",
        "symfony/psr-http-message-bridge": "^2.1",
        "twilio/sdk": "^6.13",
        "vonage/client": "2.3",
        "webpatser/laravel-uuid": "^4.0",
        "xeroapi/xero-php-oauth2": "^2.12",
        "yadahan/laravel-authentication-log": "^1.5",
        "yajra/laravel-datatables-oracle": "^9.18",
        "yoco/yoco-php-laravel": "^0.1.0@beta"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "facade/ignition": "^2.3.6",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform-check": false,
        "allow-plugins": {
            "symfony/thanks": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
